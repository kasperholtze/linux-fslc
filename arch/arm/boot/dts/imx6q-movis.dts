                                MX6QDL_PAD_SD3_DAT6__GPIO6_IO18                 0x80000000
                                MX6QDL_PAD_SD3_DAT5__GPIO7_IO00                 0x80000000
                                MX6QDL_PAD_SD3_DAT4__GPIO7_IO01                 0x80000000
                                MX6QDL_PAD_SD3_CMD__GPIO7_IO02                  0x80000000
                                MX6QDL_PAD_SD3_CLK__GPIO7_IO03                  0x80000000
                                MX6QDL_PAD_SD3_DAT2__GPIO7_IO06                 0x80000000
                                MX6QDL_PAD_SD3_DAT3__GPIO7_IO07                 0x80000000 /* upto GPIO expander */
				MX6QDL_PAD_SD3_RST__GPIO7_IO08                  0x80000000 /*super cap*/
                        >;
                };

		pinctrl_pcie: pcie {
			fsl,pins = <
				MX6QDL_PAD_GPIO_16__GPIO7_IO11                  0x80000000
			>;
		};
	};
	
};

